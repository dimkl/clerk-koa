{
  "name": "@dimkl/clerk-koa",
  "version": "0.0.1",
  "description": "Clerk server SDK for usage with Koa",
  "keywords": [
    "clerk",
    "koa"
  ],
  "homepage": "https://clerk.com/",
  "bugs": {
    "url": "https://github.com/dimkl/clerk-koa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimkl/clerk-koa.git"
  },
  "license": "MIT",
  "author": {
    "name": "Clerk, Inc.",
    "email": "dimitris.klouvas@gmail.com",
    "url": "git+https://github.com/dimkl/clerk-koa.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "dev": "tsup --watch",
    "dev:publish": "npm run dev -- --env.publish",
    "publish:local": "npx yalc push --replace  --sig",
    "clean": "rimraf ./dist",
    "lint": "eslint src/ --ext .ts",
    "test": "jest",
    "test:cache:clear": "jest --clearCache --useStderr",
    "test:ci": "jest --maxWorkers=70%"
  },
  "dependencies": {
    "@clerk/backend": "1.0.0-beta.25",
    "@clerk/shared": "2.0.0-beta.17",
    "koa-compose": "^4.1.0",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.15.0",
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsup": "*",
    "typescript": "*"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
